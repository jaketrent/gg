@font-face
  font-family: 'dice'
  src:url('dice.eot')
  src:url('dice.eot?#iefix') format('embedded-opentype'),
    url('dice.woff') format('woff'),
    url('dice.ttf') format('truetype'),
    url('dice.svg#dice') format('svg')
  font-weight: normal
  font-style: normal

.icon-die-3
.icon-die-2
.icon-die-1
.icon-die-A
.icon-die-H
.icon-die-E
.icon-star
  font-family: 'dice'
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1
  -webkit-font-smoothing: antialiased

.icon-die-3:before
  content: "\e003"

.icon-die-2:before
  content: "\e004"

.icon-die-1:before
  content: "\e005"

.icon-die-A:before
  content: "\e002"

.icon-die-H:before
  content: "\e000"

.icon-die-E:before
  content: "\e001"

// CC3Attr - P.J. Onori, from The Noun Project
.icon-star:before
  content: "\e006"
